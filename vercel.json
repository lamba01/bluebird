{
  "redirects": [
    {
      "source": "/webmail",
      "destination": "https://bluebirdconsultltd.com:2096",
      "permanent": true
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
